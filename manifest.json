{
  "name": "Bilibili next play",
  "description": "An extension to intelligent play next video on bilibili.",
  "version": "0.0.3",
  "manifest_version": 3,
  "icons": {
    "16": "icons/bilibili_red16.png",
    "32": "icons/bilibili_red32.png",
    "48": "icons/bilibili_red48.png",
    "128": "icons/bilibili_red128.png"
  },
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": "icons/bilibili_red128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "webNavigation",
    "tabs",
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "https://www.bilibili.com//*"
  ]
}